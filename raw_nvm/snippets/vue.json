  "Vue single file component": {
    "prefix": "vuesfc",
    "body": [
      "<template>",
      "  <div class=\"$0\"></div>",
      "</template>",
      "",
      "<script lang=\"ts\">",
      "import { defineComponent } from '@vue/composition-api'",
      "",
      "export default defineComponent({",
      "  name: '$0',",
      "  components: {},",
      "  props: {},",
      "",
      "  setup() {",
      "    return {}",
      "  },",
      "})",
      "</script>",
      "",
      "<style lang=\"scss\" scoped>",
      ".$0,",
      "[class*='$0--'] {",
      "}",
      "</style>"
    ],
    "description": "Vue single file component"
  },
  "Vue CPT": {
    "prefix": "vuecpt",
    "body": [
      "<template></template>",
      "",
      "<script>",
      "import Vue from 'vue'",
      "",
      "export default Vue.extend({",
      "  name: '',",
      "  data() {",
      "    return {}",
      "  },",
      "",
      "  props: [],",
      "",
      "  components: {},",
      "})",
      "</script>",
      "",
      "<style lang=\"scss\" scoped></style>",
      ""
    ],
    "description": "Vue CPT"
  },
  "Print to console": {
