{
  "SASS Include": {
    "prefix": "inc",
    "body": ["@include $1"]
  },
  "SASS MQ": {
    "prefix": "mq",
    "body": ["@include mq($1) {", "  $2", "}"]
  },
  "SASS Extend": {
    "prefix": "ext",
    "body": ["@extend %$1;"]
  },
  "Fluid": {
    "prefix": "fluid",
    "body": ["// prettier-ignore", "@include fluid($1,(xxs: $2, xxl: $3));", ""]
  },
  "VDeep": {
    "prefix": "deep",
    "body": ["::v-deep "]
  }
}
